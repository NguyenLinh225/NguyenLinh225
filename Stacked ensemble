        "ax.axhspan(ax.get_ylim()[0], mean_roc_auc, facecolor='#fdebd0', alpha=0.4)  # Bottom\n",
        "ax.axvspan(mean_accuracy, ax.get_xlim()[1], facecolor='#d6eaf8', alpha=0.4)  # Right\n",
